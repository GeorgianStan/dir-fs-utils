{
  "name": "dir-fs-utils",
  "version": "1.1.2",
  "description": "A NodeJS library that creates several utility functions over the native fs module for directories",
  "main": "./dist/dir-utils.js",
  "types": "./dist/dir-utils.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start:dev": "webpack --mode=development --watch --progress",
    "build:prod": "webpack --mode=production",
    "prepublishOnly": "webpack --mode=production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest --runInBand",
    "test:unit": "jest .*/.*.unit.spec.ts --runInBand",
    "test:integration": "jest .*/.*.integration.spec.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeorgianStan/dir-fs-utils.git"
  },
  "keywords": [
    "folder",
    "size",
    "directory",
    "fs utils",
    "folder-utils",
    "directory-utils",
    "typescript"
  ],
  "author": "Stan Georgian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GeorgianStan/dir-fs-utils/issues",
    "email": "georgian.stan8@gmail.com"
  },
  "homepage": "https://github.com/GeorgianStan/dir-fs-utils#readme",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {}
}
